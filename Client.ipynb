    "# CLIENT CELL\n",
    "import xmlrpc.client\n",
    "\n",
    "proxy = xmlrpc.client.ServerProxy(\"http://localhost:8000/\")\n",
    "num = int(input(\"Enter an integer to compute factorial: \"))\n",
    "result = proxy.factorial(num)\n",
    "print(f\"Factorial of {num} is:\\n{result}\")\n"
